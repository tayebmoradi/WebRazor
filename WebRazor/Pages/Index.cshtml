@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}


<table class="table table-striped table-bordered text-center">
    <thead>
        <tr>
            <th>ردیف</th>
            <th>نام</th>
            <th>نام خانوادگی</th>
            <th>شماره موبایل</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Users)
        {
            <tr>
                <td >@item.Id</td>
                <td >@item.Name</td>
                <td >@item.Family</td>
                <td >@item.PhoneNumber</td>
            </tr>
        }
    </tbody>
</table>

